{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        },
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "src/components/icons.js",
        "src/test/connect.ts",
        "src/data/photo/index.js",
        "src/app/(admin)/layout.js",
        "src/app/(admin)/admin/page.js",
        "src/components/ProjectsContainer.jsx",
        "src/components/ProjectCoverSelector.jsx",
        "src/components/ProjectCard.jsx",
        "src/components/PhotosContainer.jsx",
        "src/components/PhotoPreviewContainer.jsx",
        "src/components/PhotoContainer.jsx",
        "src/components/PageContentContainer.jsx",
        "src/components/NavMenu.jsx",
        "src/components/Nav.jsx",
        "src/components/AdminMenu.jsx",
        "src/components/AdditionalInfoContainer.jsx",
        "src/components/Spinner.jsx",
        "src/site/paths.js",
        "src/app/(admin)/admin/projects/page.jsx",
        "prisma/seed.js",
        "prisma/seed-data/projects.js",
        "prisma/seed-data/tags.js",
        "src/app/(admin)/admin/projects/[projectId]/page.jsx",
        "src/site/ThemeProvider.jsx",
        "src/components/forms/TagForm.jsx",
        "src/components/forms/ProjectForm.jsx",
        "src/app/layout.jsx",
        "src/app/(static)/layout.jsx",
        "src/app/(static)/page.jsx",
        "src/app/(static)/projects/[projectId]/page.jsx",
        "src/data/project/index.js",
        "src/app/error.jsx",
        "src/lib/utils.js"
    ],
    "exclude": ["node_modules"]
}
